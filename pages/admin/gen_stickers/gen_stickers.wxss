.tab{
  height: auto;
  width: 80%;
  background-color: #fff;
  border-radius: 15rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
  padding-top: 20rpx;
  padding-bottom:20rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
}
.row{
  margin: 0 auto;
  height: max-content;
  width: auto;
}
.row-title{
  height: 100%;
}
.his-title{
  font-size: 25rpx;
margin: auto;
width: 80%;
color: #353535;
margin:30rpx auto
}
.btn-group{
  margin: auto;
  width: 90%;
  height: max-content;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.btn{
  font-size: 25rpx;
  color: #8d8d8d;
  width: 160rpx;
  height: 60rpx;
  border: #e9e9e9 3rpx solid;
  text-align: center;
  border-radius: 10rpx;
  line-height: 60rpx;
  margin-left: 25rpx;
}
.btn-strong{
  border: #EA5419 3rpx solid;
  color: #fff;
  background-color: #EA5419;
}
.btn-hover{
  opacity:0.7 !important;
}
.collapse{
  background-color: #fff;
  width: 85%;
  margin: auto;
  border-radius:15rpx ;
  padding: 15rpx 0rpx;
  margin:20rpx auto;
}
.empty-bar{
  padding-top:40rpx ;
  border-radius: 15rpx;
  height: 240rpx;
  width:85%;
  margin: auto;
  background-color: #fff;
}
.empty-bar image{
  width: 120rpx;
  height: auto;
}
.empty-bar view{
  color: #666666;
  margin-top: 30rpx;
  font-size: 22rpx;
}