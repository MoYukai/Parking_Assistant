page{
  background-color: #F0F0F0;
}
.bg-top{
  width: 100%;
  height: 50rpx;
  background-image: linear-gradient(180deg, #F2AC3C 0%, #EC6E2F 100%);
  position: relative;
  padding-top: 50rpx;
}
.bg-top2{
  width: 100%;
  height: auto;
}
.tab{
  height:max-content;
  width: 85%;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 15rpx;
  padding-bottom: 30rpx;
  padding-top:20rpx;
  padding-bottom: 20rpx;
  margin-bottom: 22rpx;
}
.row{
  height:95rpx;
  width: 90%;
  margin: 0 auto;
}
.btn{
  font-size: 22rpx;
  color: #EA5419;
  width: 135rpx;
  height: 55rpx;
  border: #EA5419 3rpx solid;
  text-align: center;
  border-radius: 10rpx;
  line-height: 55rpx;
  margin-left: 25rpx;
}
.btn-strong{
  color: #fff;
  background-color: #EA5419;
}
.card{
  margin: 0 auto;
  width: 160rpx;
  height: 48rpx;
  border: 3rpx #EA5419 solid;
  border-radius: 10rpx;
  color: #EA5419;
  font-size:27rpx;
  text-align:center;
  line-height: 48rpx;
}
.phone{
  font-size: 28rpx;
  color: #646464;
}
.time{
  padding-left: 5rpx;
  color:#8D8D8D;
  font-size: 16rpx;
}
.col-b{
  margin-left: 20rpx;
}
.btn-hover{
  opacity:0.7 !important;
}
.null-tab{
  height: 260rpx;
}
.null-box{
  width: 80%;
  height: max-content;
}
.null-code{
  height: 250rpx;
}
.null-code image{
  width: 110rpx;
}
.null-code-text{
  color: #a7a7a7;
  font-size:25rpx;
}
.oqp{
  width: auto;
  height: 90rpx;
}